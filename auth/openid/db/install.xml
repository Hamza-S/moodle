<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="auth/openid/db" VERSION="20170217" COMMENT="XMLDB file for Moodle auth/openid"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="auth_openid_idp" COMMENT="Identity Providers">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="The time this record was created."/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="The time this record was modified."/>
        <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="The user who last modified this record."/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="Identity Provider Name. Displayed on login page."/>
        <FIELD NAME="image" TYPE="text" NOTNULL="false" SEQUENCE="false" COMMENT="URL to an image representing this identity provider."/>
        <FIELD NAME="discoveryendpoint" TYPE="text" NOTNULL="true" SEQUENCE="false" COMMENT="URL containing information about the end points and scopes supported by this provider."/>
        <FIELD NAME="authorizationendpoint" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="tokenendpoint" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="userinfoendpoint" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="revocationendpoint" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="scopessupported" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
        <FIELD NAME="clientid" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="clientsecret" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
