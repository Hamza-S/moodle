{{< core_form/element-template-inline }}
    {{$element}}
        {{^element.frozen}}
        <input type="submit"
                class="btn
                    {{^element.iscancel}}btn-primary{{/element.iscancel}}
                    {{#element.iscancel}}btn-secondary{{/element.iscancel}}
                    {{#error}} btn-danger {{/error}}"
                name="{{element.name}}"
                id="{{element.id}}"
                value="{{element.value}}"
                {{{element.attributes}}} >
        {{/element.frozen}}
    {{/element}}
{{/ core_form/element-template-inline }}
