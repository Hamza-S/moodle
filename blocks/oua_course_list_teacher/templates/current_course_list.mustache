<li class="coursecol course-header col-xs-12">
    <div class="course-header-content {{coursenameclass}} ">
        <div class="course-class-logo hidden-xs {{brandlogoclass}}">
            {{#cobrandinglogo}}
                <span class="logo-course"><img class="logo img-responsive" src="{{{cobrandinglogo}}}" alt="{{cobrandingname}}"/></span>
            {{/cobrandinglogo}}
        </div>
        <div class="course-class-summary">
            <!--<div class="progress-bar-show-weeks">-->
                <div class="course-name">
                    <a href="{{courseviewurl}}">{{{coursename}}}</a>
                    {{#nextactivityname}}
                        <div class="whatsnext">{{#str}}whatsnextx, block_oua_course_list_teacher, {{nextactivityname}}{{/str}}</div>
                    {{/nextactivityname}}
                </div>
                <div class="my-course">
                    <div class="adminbuttons {{courselinkclass}}">
                        <a class="managemessages" href="{{{managemessageslink}}}"><i class="fa fa-bullhorn"></i><span>{{#str}}managemessages, block_oua_course_list_teacher{{/str}}</span></a>
                        <a class="gotogrades" href="{{{finalgradelink}}}"><i class="fa fa-table"></i><span>{{#str}}gotogrades, block_oua_course_list_teacher{{/str}}</span></a>
                    </div>
                </div>
            <!--</div>-->
        </div>
    </div>
</li>
