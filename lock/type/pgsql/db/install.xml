<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="lock/type/pgsql/db" VERSION="20130707" COMMENT="XMLDB file for Moodle lock/type/pgsql"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="locktype_pgsql" COMMENT="Maintains a list of mappings of lock keys to integers.">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="key" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="The unique key for this lock. This should be in frankenstyle format, but this is up to the caller."/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="key_idx" UNIQUE="true" FIELDS="key" COMMENT="Index on key"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>